{
	"name": "JieDark",
	"colors": {
		"editor.background": "#1E1F22",
		"editor.foreground": "#6f737a",
		"sideBar.background": "#2B2D30",
		"editor.lineHighlightBackground": "#26282D",
		"editorBracketHighlight.foreground1": "#6f737a",
		"editorBracketHighlight.foreground2": "#6f737a",
		"editorBracketHighlight.foreground3": "#6f737a",
		"editorBracketHighlight.foreground4": "#6f737a",
		"editorBracketHighlight.foreground5": "#6f737a",
		"editorBracketHighlight.foreground6": "#6f737a",
		"editorBracketHighlight.unexpectedBracket.foreground": "#A31515",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#999999",
		"parameter": {
			"foreground": "#969696",
			"underline": true
		},
		"property": {
			"foreground": "#c87dbb",
		},
		"enumMember": {
			"foreground": "#c87dbb",
			"fontStyle": "italic",
		},
		"method.declaration": "#6aab73",
		"function.declaration": "#6aab73",
		"class.declaration": "#6aab73",
		"enum.declaration": "#6aab73",
		"property.static": {
			"fontStyle": "bold"
		},
		"keyword.documentation": {
			"foreground": "#969696"
		},
	},
	"tokenColors": [
		// java
		// 注解
		// 类、抽象类、接口
		// 构造方法、普通方法（私有、保护、公开、默认）、静态方法
		// 实例变量、类变量
		// 构造方法参数、方法参数、静态方法参数
		// 方法内变量
		{
			"name": "Comments",
			"scope": [
				"comment.line.double-slash.java",
				"comment.block.java",
			],
			"settings": {
				"foreground": "#6f7370"
			}
		},
		{
			"name": "KeywordControl",
			"scope": [
				"variable.language.java",
				"keyword.control.java",
				"keyword.control.catch-exception.java",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Annotation",
			"scope": [
				"storage.type.annotation.java",
			],
			"settings": {
				"foreground": "#b3ae50"
			}
		},
		{
			"name": "Contsant",
			"scope": [
				"constant.language.java",
				"constant.numeric.integer.java",
				"constant.numeric.float.java",
				"constant.numeric.hex.java",
				"constant.numeric.octal.java",
				"constant.numeric.binary.java",
				"constant.numeric.hex-float.java",
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.double.java",
			],
			"settings": {
				"foreground": "#888888"
			}
		},
	]
}