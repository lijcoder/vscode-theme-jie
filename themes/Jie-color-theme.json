{
	"name": "LiJie",
	"colors": {
		"editor.background": "#F3F3F3",
		"editor.foreground": "#333333",
		"sideBar.background": "#EEEEEE",
		"editor.lineHighlightBackground": "#FFFFFF",
		"editorBracketHighlight.foreground1": "#333333",
		"editorBracketHighlight.foreground2": "#333333",
		"editorBracketHighlight.foreground3": "#333333",
		"editorBracketHighlight.foreground4": "#333333",
		"editorBracketHighlight.foreground5": "#333333",
		"editorBracketHighlight.foreground6": "#333333",
		"editorBracketHighlight.unexpectedBracket.foreground": "#A31515",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#0133B3",
		"parameter": {
			"foreground": "#0133B3",
			"underline": true
		},
		"property": {
			"foreground": "#871094",
		},
		"method.declaration": "#0000FF",
		"property.static": {
			"fontStyle": "bold"
		},
		"keyword.documentation": {
			"foreground": "#000000"
		},
		"enum.declaration": "#0000FF",
		"enumMember": {
			"foreground": "#b418c6",
			"fontStyle": "italic",
		},
	},
	"tokenColors": [
		// java
		// 注解
		// 类、抽象类、接口
		// 构造方法、普通方法（私有、保护、公开、默认）、静态方法
		// 实例变量、类变量
		// 构造方法参数、方法参数、静态方法参数
		// 方法内变量
		{
			"name": "Comments",
			"scope": [
				"comment.line.double-slash.java",
				"comment.block.java",
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "KeywordControl",
			"scope": [
				"variable.language.java",
				"keyword.control.java",
				"keyword.control.catch-exception.java",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Annotation",
			"scope": [
				"storage.type.annotation.java",
			],
			"settings": {
				"foreground": "#9e880d"
			}
		},
		{
			"name": "Contsant",
			"scope": [
				"constant.language.java",
				"constant.numeric.integer.java",
				"constant.numeric.float.java",
				"constant.numeric.hex.java",
				"constant.numeric.octal.java",
				"constant.numeric.binary.java",
				"constant.numeric.hex-float.java",
			],
			"settings": {
				"foreground": "#5C6B7C"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.double.java",
			],
			"settings": {
				"foreground": "#5C6B7C"
			}
		},
	]
}