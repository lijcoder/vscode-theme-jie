{
  "name": "vscode-theme-jie",
  "displayName": "vscode theme Jie",
  "description": "An eye-friendly theme designed for long coding sessions",
  "version": "0.0.4",
  "publisher": "lijcoder",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijcoder/vscode-theme-jie"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Themes",
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Jie Light",
        "uiTheme": "vs",
        "path": "./themes/Jie-light-color-theme.json"
      },
      {
        "label": "Jie Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Jie-dark-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "java",
        "aliases": [
          "java",
          "java"
        ],
        "extensions": [
          ".java"
        ],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "java",
        "scopeName": "source.java",
        "path": "./syntaxes/java.tmLanguage"
      }
    ]
  }
}
