{
  "name": "vscode-theme-jie",
  "displayName": "vscode theme Jie",
  "description": "An eye-friendly theme designed for long coding sessions",
  "version": "0.0.13",
  "publisher": "lijcoder",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijcoder/vscode-theme-jie"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Themes",
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Jie Light",
        "uiTheme": "vs",
        "path": "./themes/light-color-theme.json"
      },
      {
        "label": "Jie Light High",
        "uiTheme": "vs",
        "path": "./themes/light-hight-color-theme.json"
      },
      {
        "label": "Jie Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "java",
        "aliases": [
          "java",
          "java"
        ],
        "extensions": [
          ".java"
        ],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "java",
        "scopeName": "source.java",
        "path": "./syntaxes/java.tmLanguage"
      }
    ]
  },
  "scripts": {
    "build": "tsx src/build.ts",
    "watch:theme": "tsx src/watch.ts",
    "dev": "npm run build && npm run watch:theme",
    "vsce:package": "npx vsce package",
    "vsce:publish:patch": "npx vsce publish patch"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "chokidar": "^4.0.3",
    "@types/chokidar": "^2.1.3",
    "tsx": "^4.20.3"
  }
}
